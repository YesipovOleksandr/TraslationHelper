@{
    ViewData["Title"] = "Home Page";
}

<form asp-controller="Home" asp-action="Upload" method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label for="googleDocUrl">Url Google document</label>
            <input type="text" class="form-control" id="googleDocUrl" name="googleDocUrl" required />
    </div>
    <div class="form-group">
        <label for="htmlFile">HTML File</label>
        <input type="file" class="form-control-file" id="htmlFile" name="htmlFile" accept=".html" required />
    </div>
    <button type="submit" class="btn btn-primary">Upload</button>
</form>